<!DOCTYPE html>

<!--[if IE]><![endif]-->
<!--[if IE 8 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie8"><![endif]-->
<!--[if IE 9 ]><html dir="{{ direction }}" lang="{{ lang }}" class="ie9"><![endif]-->
<!--[if (gt IE 9)|!(IE)]><!-->
<html dir="{{ direction }}" lang="{{ lang }}">
<!--<![endif]-->
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>{{ title }}</title>
    <base href="{{ base }}" />
    {% if description %}
        <meta name="description" content="{{ description }}" />
    {% endif %}
    {% if keywords %}
        <meta name="keywords" content="{{ keywords }}" />
    {% endif %}
    <link rel="preload" href="catalog/view/theme/qweb/fonts/icomoon.ttf?p0kfys"  as="font" type="font/ttf" crossorigin>

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/css/bootstrap.min.css" integrity="sha384-B0vP5xmATw1+K9KRQjQERJvTumQW0nPEzvF6L/Z6nronJ3oUOFUFpCjEUQouq2+l" crossorigin="anonymous">
    <link href="catalog/view/theme/qweb/css/style.css" rel="stylesheet">
    <script src="catalog/view/theme/qweb/js/jquery-3.4.1.min.js?ver1.0"></script>
    <script src="catalog/view/theme/qweb/js/jquery-migrate-1.4.1.min.js?ver1.0"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-Piv4xVNRyMGpqkS2by6br4gNJ7DXjqk09RmUpJ8jgGtD7zP9yug3goQfGII0yAns" crossorigin="anonymous"></script>
    <link rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous"/>

    {% for style in styles %}
        <link href="{{ style.href }}" type="text/css" rel="{{ style.rel }}" media="{{ style.media }}" />
    {% endfor %}
    {% for script in scripts %}
        <script src="{{ script }}" type="text/javascript"></script>
    {% endfor %}

    <script src="catalog/view/javascript/common.js" type="text/javascript"></script>
    {% for link in links %}
        <link href="{{ link.href }}" rel="{{ link.rel }}" />
    {% endfor %}
    {% for analytic in analytics %}
        {{ analytic }}
    {% endfor %}
    
</head>
<body>
<div class="sidebar">
    <div class="sidebar-inner">

        <div class="sidebar-nav">
            <div class="main-nav">
                <a href="#" class="main-nav__link">{{ text_catalog_of_products }} <span class="icon-down-arrow"></span></a>
            </div>

            <div class="main-subnav-wrapper"  id="main-subnav-wrapper-2"></div>

            <ul class="secondary-nav flex-wrap d-flex align-items-center">
                <li class="secondary-nav-item">
                    <a class="secondary-nav-link" href="{{ about_us }}">{{ text_about_us }}</a>
                </li>
                <li class="secondary-nav-item">
                    <a class="secondary-nav-link" href="{{ delivery_and_payment }}">{{ text_delivery_and_payment }}</a>
                </li>
                <li class="secondary-nav-item">
                    <a class="secondary-nav-link" href="{{ contact }}">{{ text_contact_page }}</a>
                </li>
            </ul>
        </div>

        {{ search_mobile }}

        <div class="sidebar-info d-flex flex-wrap">
            <ul class="header-phone">
                <li class="header-phone__item"><span class="icon-kyivstar"></span><a href="tel:{{ config_kyivstar |replace({'-':"",'+':"",'(':"",')':"",' ':"","38":""}) }}">{{ config_kyivstar }}</a></li>
                <li class="header-phone__item"><span class="icon-lifecell"></span><a href="tel:{{ config_life |replace({'-':"",'+':"",'(':"",')':"",' ':"","38":""}) }}">{{ config_life }}</a></li>
                <li class="header-phone__item"><span class="icon-mts"></span><a href="tel:{{ config_vodafone |replace({'-':"",'+':"",'(':"",')':"",' ':"","38":""}) }}">{{ config_vodafone }}</a></li>
            </ul>
            <ul class="header-info">
                <li class="header-info__item header-info__email"><a href="mailto:{{ config_email }}">{{ config_email }}</a></li>
                <li class="header-info__item"><a href="{{ contact }}">{{ text_look_at_map }}</a></li>
                <li class="header-info__item">{{ config_open }}</li>
            </ul>
        </div>

    </div>
</div>
<div class="main-wrapper">

    <!-- BEGIN HEADER -->
    <header class="header" id="header">
        <div id="top-links" class="nav">
            <div class="container">
                <ul class="top_acc">
                    <li class="li-wh"><a href="{{ wishlist }}" id="wishlist-total"><i class="far fa-star"></i> <span class="count_head">{{ text_wishlist }}</span></a></li>
                    <li class="ml-3 li-wh"><a href="{{ compare }}" id="compare-total"><i class="fa fa-exchange"></i> <span class="count_head">{{ text_compare }}</span></a></li>
                    <li class="ml-4 dropdown li-wh"><a href="{{ account }}" title="{{ text_account }}" class="dropdown-toggle" data-toggle="dropdown">
                    <i class="far fa-user"></i> <span class="caret"></span></a>
                    <ul class="dropdown-menu dropdown-menu-right">
                        {% if logged %}
                        <li><a href="{{ account }}">{{ text_account }}</a></li>
                        <li><a href="{{ order }}">{{ text_order }}</a></li>
                        <li><a href="{{ logout }}">{{ text_logout }}</a></li>
                        {% else %}
                        <li><a href="{{ register }}">{{ text_register }}</a></li>
                        <li><a href="{{ login }}">{{ text_login }}</a></li>
                        {% endif %}
                    </ul>
                    </li>
                </ul>
            </div>
        </div>
        <div class="header-top">
            <div class="container container-fluid d-flex flex-row justify-content-between align-items-center">
                <a href="{{ home }}" class="logo">
                    <img src="{{ logo }}" alt="{{ name }}">
                </a>
                <ul class="header-phone d-none d-md-block">
                    <li class="header-phone__item"><span class="icon-kyivstar"></span><a href="tel:{{ config_kyivstar |replace({'-':"",'+':"",'(':"",')':"",' ':"","38":""}) }}">{{ config_kyivstar }}</a></li>
                    <li class="header-phone__item"><span class="icon-lifecell"></span><a href="tel:{{ config_life |replace({'-':"",'+':"",'(':"",')':"",' ':"","38":""}) }}">{{ config_life }}</a></li>
                    <li class="header-phone__item"><span class="icon-mts"></span><a href="tel:{{ config_vodafone |replace({'-':"",'+':"",'(':"",')':"",' ':"","38":""}) }}">{{ config_vodafone }}</a></li>
                </ul>
                <ul class="header-info d-none d-lg-block">
                    <li class="header-info__item header-info__email"><a href="mailto:{{ config_email }}">{{ config_email }}</a></li>
                    <li class="header-info__item"><a href="{{ contact }}">{{ text_look_at_map }}</a></li>
                    <li class="header-info__item">{{ config_open }}</li>
                </ul>
                {{ cart }}
                <a href="#" class="mobile-trigger d-lg-none">
                        <span class="hamburger hamburger--collapse">
                            <span class="hamburger-box">
                                <span class="hamburger-inner"></span>
                            </span>
                        </span>
                </a>

            </div>
        </div>
        <div class="header-nav d-none d-lg-block">
            <div class="container container-fluid d-flex align-items-center justify-content-between">
                <div class="header-nav__left d-flex align-items-center ">
                    <div class="main-nav">
                        <a href="#" class="main-nav__link">{{ text_catalog_of_products }} <span class="icon-down-arrow"></span></a>
                    </div>
                    <ul class="secondary-nav flex-nowrap d-flex align-items-center">
                        <li class="secondary-nav-item">
                            <a class="secondary-nav-link" href="{{ about_us }}">{{ text_about_us }}</a>
                        </li>
                        <li class="secondary-nav-item">
                            <a class="secondary-nav-link" href="{{ delivery_and_payment }}">{{ text_delivery_and_payment }}</a>
                        </li>
                        <li class="secondary-nav-item">
                            <a class="secondary-nav-link" href="{{ contact }}">{{ text_contact_page }}</a>
                        </li>
                    </ul>
                </div>
                {{ search }}
                {{ menu }}
            </div>
        </div>

    </header>
    <!-- HEADER EOF   -->
    <main class="content">



